{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://acc-sync.vercel.app/api/$1"
      },
      {
        "source": "/auth/(.*)",
        "destination": "https://acc-sync.vercel.app/auth/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }